[package]
name = "suricata-derive"
version = "@PACKAGE_VERSION@"
edition = "2021"
description = "Derive macros for Suricata"
license = "GPL-2.0-only"
repository = "http://github.com/OISF/suricata.git"

[lib]
proc-macro = true
path = "@e_rustdir@/derive/src/lib.rs"

[dependencies]
proc-macro-crate = "= 1.1.0"
proc-macro2 = "1.0.36"
quote = "1.0"
syn = "1.0"
