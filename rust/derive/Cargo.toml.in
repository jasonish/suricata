[package]
name = "suricata-derive"
version.workspace = true
edition = "2021"
description = "Derive macros for Suricata"
license = "GPL-2.0-only"
repository = "http://github.com/OISF/suricata.git"
@cargo_publish_comment@publish = false

[lib]
proc-macro = true
@cargo_publish_comment@path = "@e_rustdir@/derive/src/lib.rs"

[dependencies]
proc-macro-crate = "= 1.1.0"
proc-macro2 = "1.0.36"
quote = "1.0"
syn = "1.0"
